  <ItemGroup>
    <PackageReference Include="Cross.BuildingBlocks.Core" Version="1.1.1" />
  </ItemGroup>

  <ItemGroup>
    <None Include="..\..\LICENSE.md" Pack="true" PackagePath="LICENSE.md"/>
